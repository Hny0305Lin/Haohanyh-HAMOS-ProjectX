<?xml version="1.0" encoding="utf-8"?>
<LinearLayout xmlns:android="http://schemas.android.com/apk/res/android"
    xmlns:app="http://schemas.android.com/apk/res-auto"
    xmlns:tools="http://schemas.android.com/tools"
    android:layout_width="match_parent"
    android:layout_height="match_parent"
    tools:context=".ImportActivity.FirstActivity">

    <LinearLayout
        android:layout_width="match_parent"
        android:layout_height="match_parent"
        android:orientation="vertical">


        <LinearLayout
            android:layout_width="match_parent"
            android:layout_height="480sp"
            android:orientation="horizontal">

            <LinearLayout
                android:layout_width="match_parent"
                android:layout_height="match_parent"
                android:orientation="vertical">

                <Button
                    android:id="@+id/kaiqi"
                    android:background="@drawable/button_nihao123123"
                    android:layout_width="match_parent"
                    android:layout_height="50sp"
                    android:text="@string/haohanyh_hamos_lights_project_manymain1"
                    android:textColor="@color/Pink_is_justice"
                    android:textStyle="italic" />

                <TextView
                    android:id="@+id/txtwendu"
                    android:layout_width="match_parent"
                    android:layout_height="50sp"
                    android:background="@drawable/label_customize_nihao123123"
                    android:text="" />

                <TextView
                    android:id="@+id/txtshidu"
                    android:layout_width="match_parent"
                    android:layout_height="50sp"
                    android:background="@drawable/label_customize_nihao123123"
                    android:text="" />

                <TextView
                    android:id="@+id/shijian"
                    android:layout_width="match_parent"
                    android:layout_height="50sp"
                    android:background="@drawable/label_customize_nihao123123"
                    android:text="" />


            </LinearLayout>

        </LinearLayout>

    </LinearLayout>

</LinearLayout>